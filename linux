FROM (FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_20170603],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_20170604],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_20170605],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_ANDROID.app_events_20170603],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_ANDROID.app_events_20170604],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_ANDROID.app_events_20170605],user_dim.user_properties.value.value.string_value))
WHERE event_dim.name = "first_open");



SUBCONSULTA QUE FUNCIONA
------------------------------
select * FROM (select COUNT(event_dim.params.value.string_value) AS TOTAL 
FROM (FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_20170603],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_20170604],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_20170605],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_ANDROID.app_events_20170603],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_ANDROID.app_events_20170604],user_dim.user_properties.value.value.string_value)),
(FLATTEN([cityxerpa-b57e7:es_dae_cityxerpa_ANDROID.app_events_20170605],user_dim.user_properties.value.value.string_value))
WHERE user_dim.user_properties.value.value.string_value = 'Andorra' AND event_dim.name = "ecommerce_purchase" AND event_dim.params.key = "business")
GROUP BY TOTAL
ORDER BY 1 DESC;


event_dim.params.key ===  category_name
event_dim.params.value.string_value === Mercado


SELECT user_dim.first_open_timestamp_micros, event_dim.name,event_dim.params.key,event_dim.params.value.string_value,event_dim.params.value.int_value,event_dim.params.value.float_value,event_dim.params.value.double_value
FROM (TABLE_DATE_RANGE([cityxerpa-b57e7:es_dae_cityxerpa_IOS.app_events_], TIMESTAMP('2017-03-01'), TIMESTAMP('2017-03-31'))) 
WHERE event_dim.name="ecommerce_purchase" and event_dim.params.key="business" and event_dim.params.value.string_value!="noBusiness"
GROUP BY user_dim.first_open_timestamp_micros, event_dim.name,event_dim.params.key,event_dim.params.value.string_value,event_dim.params.value.int_value,event_dim.params.value.float_value,event_dim.params.value.double_value


